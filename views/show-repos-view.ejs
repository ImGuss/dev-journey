<div class="container">
  <div class="row">
    <div class="col-sm-6">
      <div>AMOUNT: <%= repos.length %></div>
      <% repos.forEach( (repo) => { %>
        <div>CREATED AT: <%= repo.created_at %></div>
      <% }); %>
    </div>
    <div class="col-sm-6">
      <div>AMOUNT: <%= languages.length %></div>
      <% languages.forEach( (language) => { %>
        <div>CREATED AT: <%= language.created_at %></div>
      <% }); %>
    </div>
  </div>
</div>
