<div class="container">
  <div class="row">
    <div class="col-sm-4"></div>

    <div class="col-sm-4">
      <div>AMOUNT: <%= repos.length %></div>
      <br><br>
      <% repos.forEach( (repo) => { %>
        <div><%= repo.name %></div>
        <div>CREATED AT: <%= repo.created_at %></div>
        <div>LANGUAGE: <%= repo.language %></div>
        <% }); %>
    </div>

    <div class="col-sm-4"></div>
  </div>
</div>
